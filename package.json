{
  "name": "cbc-data",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "10.16.x"
  },
  "scripts": {
    "generate:typings": "ts-node ./src/generate-typings",
    "prebuild": "rimraf dist",
    "build": "yarn generate:typings && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn generate:typings && nest start",
    "start:dev": "yarn generate:typings && nest start --watch",
    "start:debug": "yarn generate:typings && nest start --debug --watch",
    "start:prod": "yarn generate:typings && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.9",
    "@nestjs/graphql": "^7.3.4",
    "@nestjs/platform-express": "^7.0.9",
    "@prisma/client": "^2.0.0-beta.3",
    "apollo-server-express": "^2.12.0",
    "dayjs": "^1.8.25",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "morphism": "^1.12.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.9",
    "@prisma/cli": "^2.0.0-beta.3",
    "@types/express": "^4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
